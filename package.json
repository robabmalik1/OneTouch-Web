  "scripts": {
    "build-css": "node-sass-chokidar src/styles/global/ -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/global/ -o src/styles/css/ --watch --recursive --usePolling",
    "tailwind": "./node_modules/.bin/tailwind build ./src/styles/tailwind.base.css -c ./tailwind.js -o ./src/styles/tailwind.css",
    "start-js": "react-scripts start",
    "start-old": "npm-run-all -p watch-css start-js",
    "start": "npm-run-all -p start-js",
    "build": "npm run tailwind && node ./purge-tailwindcss.js && cross-env GENERATE_SOURCEMAP=false && npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
